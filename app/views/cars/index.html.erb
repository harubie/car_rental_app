<h1>Welcome to Car Rentals! Your one stop shop to renting cars!</h1>
<h2>Pick a car to view details</h2>

<ul>
  <% @cars.each do |car| %>
  <li class="border border-gray-300 rounded p-4 mb-4">
    <p><%= car.title %></p>
    <p><%= car.brand %></p>
    <p><%= car.year %></p>
    <%= link_to  "View details to see availability", car_path(car) %>
  </li>
  <% end %>
</ul>
